{
  "name": "moscow_metro",
  "version": "2.1.3",
  "description": "Scalable vector moscow metro map",
  "main": "build/moscow_metro.js",
  "scripts": {
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "build": "node src/map/handler && NODE_ENV=production webpack",
    "build:watch": "node src/map/handler && NODE_ENV=development webpack --watch",
    "start": "npm run build:watch & webpack-dev-server"
  },
  "keywords": [
    "moscow_metro.js",
    "metro",
    "moscow",
    "underground",
    "svg",
    "vector"
  ],
  "author": "Viacheslav Sergeev <philosophocat@ya.ru>",
  "homepage": "https://github.com/philosophocat/moscow_metro",
  "repository": {
    "type": "git",
    "url": "http://github.com/philosophocat/moscow_metro.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@types/node": "^10.12.18",
    "babel-loader": "^8.1.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-spread": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "core-js": "^3.6.5"
  }
}
